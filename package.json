{
  "name": "@ianprime0509/jsonresume-schema",
  "version": "0.0.1",
  "description": "An unofficial fork of the JSON Resume schema, with TypeScript support.",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "scripts": {
    "build": "run-s gen-schema-types compile",
    "compile": "tsc",
    "gen-schema-types": "json2ts -i resume.schema.json -o lib/schema.d.ts && cp lib/schema.d.ts dist/lib/",
    "prepare": "run-s build",
    "run-tests": "mocha -r ts-node/register lib/**/*.spec.ts",
    "test": "run-s gen-schema-types run-tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ianprime0509/jsonresume-schema.git"
  },
  "keywords": [
    "json",
    "resume",
    "schema"
  ],
  "author": {
    "name": "Ian Johnson",
    "email": "ianprime0509@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ianprime0509/jsonresume-schema/issues"
  },
  "homepage": "https://github.com/ianprime0509/jsonresume-schema#readme",
  "dependencies": {
    "ajv": "^6.5.4"
  },
  "devDependencies": {
    "@types/chai": "^4.1.5",
    "@types/mocha": "^5.2.5",
    "chai": "^4.2.0",
    "json-schema-to-typescript": "^6.0.1",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.1"
  }
}
