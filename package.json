  "dependencies": {
    "ajv": "^6.5.4"
  },
  "devDependencies": {
    "@types/chai": "^4.1.5",
    "@types/mocha": "^5.2.5",
    "chai": "^4.2.0",
    "json-schema-to-typescript": "^6.0.1",
    "mocha": "^5.2.0",
